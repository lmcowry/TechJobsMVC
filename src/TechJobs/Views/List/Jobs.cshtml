<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{
// TODO #3 - Use the same list display from Views/Search/Index.cshtml
    @foreach (Dictionary<string, string> eachDictionary in ViewBag.jobs)
    {
        <div>
        <table class = "job-listing">
            @foreach (KeyValuePair<string, string> eachEntry in eachDictionary)
            {
                <tr>
                    <td>@eachEntry.Key</td>
                    <td>@eachEntry.Value</td>
                </tr>
            }
        </table>
            </div>
    }
}